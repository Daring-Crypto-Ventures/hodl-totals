{
  "name": "hodl-totals",
  "version": "2.0.1",
  "description": "Collection of various cryptocurrency tracking tools and scripts for Google Sheetsâ„¢.",
  "scripts": {
    "clean": "rm -rf dist",
    "compile": "genversion --es6 --semi src/version.ts && replace-in-file \"export const\" \"const\" src/version.ts && replace-in-file \";\" \"; export { version };\" src/version.ts && rollup -c && npm run manifest",
    "manifest": "cp appsscript.json dist/",
    "lint": "eslint -c .eslintrc.js .",
    "test:e2e": "npm run deploy && clasp open --webapp",
    "test:debug": "node --experimental-specifier-resolution=node --inspect node_modules/.bin/jest --runInBand",
    "test:unit": "jest --coverage",
    "test:unit:watch": "jest --verbose --watchAll",
    "test": "npm run test:unit && npm run test:e2e",
    "deploy": "clasp push",
    "build": "npm run clean && npm run lint && npm run compile"
  },
  "keywords": [
    "crypto",
    "google-sheets",
    "tax",
    "cryptotax",
    "cryptocurrency",
    "cryptocurrencies",
    "taxes",
    "gains",
    "cost-basis",
    "hodl-totals"
  ],
  "author": "dogracer <dogracer@tutanota.com>, Daring Crypto Ventures LLC",
  "homepage": "https://github.com/dogracer/hodl-totals#readme",
  "license": "MIT",
  "devDependencies": {
    "@google/clasp": "^2.4.2",
    "@ni/eslint-config-javascript": "^4.3.0",
    "@ni/eslint-config-typescript": "^4.4.0",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/babel__core": "7.1.18",
    "@types/babel__traverse": "7.14.2",
    "@types/google-apps-script": "^1.0.91",
    "@types/google.script.client-side": "^0.1.1",
    "@types/jest": "^27.4.1",
    "@types/qunit": "^1.16.37",
    "@typescript-eslint/eslint-plugin": "~5.62.0",
    "@typescript-eslint/parser": "~5.62.0",
    "@web/rollup-plugin-html": "^1.11.0",
    "browserslist": "^4.24.4",
    "coveralls": "^3.1.1",
    "eslint": "^8.38.0",
    "eslint-plugin-import": "^2.27.5",
    "genversion": "^3.2.0",
    "eslint-plugin-jest": "~27.2.1",
    "glob": "^8.0.3",
    "jest": "^27.5.1",
    "json-schema": ">=0.4.0",
    "replace-in-file": "^8.3.0",
    "rollup": "^4.32.0",
    "ts-jest": "^27.1.4",
    "typescript": "3.8.2"
  }
}
